{
  "name": "ahwr-document-generator-backend",
  "version": "0.0.0",
  "description": "ahwr-document-generator-backend",
  "main": "src/index.js",
  "type": "module",
  "engines": {
    "node": ">=22.21.1"
  },
  "scripts": {
    "dev": "NODE_ENV=development npm run server:watch",
    "dev:debug": "NODE_ENV=development npm run server:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"**/*.{cjs,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{cjs,js,json,md}\"",
    "git:pre-commit-hook": "npm run format:check && npm run lint && npm test",
    "lint": "eslint --cache --cache-strategy content \"**/*.{cjs,js}\"",
    "lint:fix": "npm run lint -- --fix",
    "postinstall": "npm run setup:husky",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module",
    "start": "NODE_ENV=production node --use-strict .",
    "test": "jest --runInBand",
    "test:watch": "jest --coverage=false --onlyChanged --watch --runInBand"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@aws-sdk/client-s3": "3.982.0",
    "@defra/cdp-auditing": "0.4.0",
    "@defra/hapi-secure-context": "0.1.3",
    "@defra/hapi-tracing": "1.22.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/boom": "10.0.1",
    "@hapi/hapi": "21.4.3",
    "aws-embedded-metrics": "4.2.0",
    "aws4": "1.13.2",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "ffc-ahwr-common-library": "3.5.4",
    "global-agent": "3.0.0",
    "hapi-pino": "12.1.0",
    "hapi-pulse": "3.0.1",
    "http-status-codes": "2.3.0",
    "https-proxy-agent": "7.0.6",
    "joi": "17.13.3",
    "moment": "2.29.4",
    "mongo-locks": "3.0.2",
    "mongodb": "6.17.0",
    "node-fetch": "3.3.2",
    "pdfmake": "0.2.20",
    "pino": "9.7.0",
    "pino-pretty": "13.0.0",
    "undici": "7.20.0"
  },
  "overrides": {
    "brace-expansion": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "7.27.4",
    "@babel/plugin-transform-modules-commonjs": "7.27.1",
    "@babel/preset-env": "7.27.2",
    "@types/jest": "29.5.14",
    "autoprefixer": "10.4.21",
    "babel-jest": "30.1.2",
    "babel-plugin-transform-import-meta": "2.3.3",
    "eslint": "9.29.0",
    "husky": "9.1.7",
    "jest": "30.1.2",
    "jest-junit": "13.0.0",
    "mongodb-memory-server": "10.2.3",
    "neostandard": "0.12.1",
    "nodemon": "3.1.10",
    "npm-run-all": "4.1.5",
    "prettier": "3.5.3"
  }
}
